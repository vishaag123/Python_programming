    "def fibonacci(n,r):\n",
    "    if n<=1:\n",
    "        r[n]=n\n",
    "        return n\n",
    "    if n in r:\n",
    "        return r[n]\n",
    "    else:\n",
    "        r[n]=fibonacci(n-1,r)+fibonacci(n-2,r)\n",
    "        return r[n]\n",
    "r={}\n",
    "print(fibonacci(10,r))\n",
    "print(r)"
